
@{
    ViewBag.Title = "Index";
}
<br/>
@using (Html.BeginForm("Index", "PublicStorage",FormMethod.Post, new {}))
{
    if(ViewBag.Error != null)
     {
         @ViewBag.Error
         <br/>
    }

    <div class="row">
        <div class="form-group col-md-offset-4 col-md-4">
            <label for="AnonimOwner">Your name </label>
            <input type="text" class="form-control" name="AnonimOwner" id="AnonimOwner" placeholder="Enter your name here">
        </div>
    </div>
    <div class="row">
        <input type="submit" value="Submit" class="col-md-offset-7 btn btn-success" />
    </div>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
